{
  "name": "youtube",
  "version": "1.0.1",
  "description": "Small app to downloads youtube videos",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "win32": "electron-builder --win --ia32",
    "win64": "electron-builder --win --x64",
    "linux32": "electron-builder --linux --ia32",
    "linux64": "electron-builder --linux --x64"
  },
  "build": {
    "appId": "fr.mle-moni.renouv",
    "win": {
      "target": [
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "author": "LE MONIES DE SAGAZAN Mayeul",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^4.4.0"
  }
}
